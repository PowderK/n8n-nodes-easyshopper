{
  "name": "n8n-nodes-easyshopper",
  "version": "0.1.4",
  "description": "n8n Community Node fÃ¼r EasyShopper Einkaufslisten-Verwaltung",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "easyshopper",
    "shopping",
    "groceries",
    "einkaufsliste"
  ],
  "license": "MIT",
  "homepage": "https://github.com/benni/n8n-nodes-easyshopper",
  "author": {
    "name": "EasyShopper Integration",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benni/n8n-nodes-easyshopper.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "cp src/nodes/EasyShopper/easyshopper.svg dist/nodes/EasyShopper/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier src --write",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CREDENTIALS_GUIDE.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EasyShopperApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EasyShopper/EasyShopper.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=8.0.0"
  }
}
